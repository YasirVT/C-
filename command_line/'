int main(int argc, char** argv)
{
    cout << "You have entered " << argc
         << " arguments:" << "\n";
	int sum = 0;
//    for (int i = 0; i < argc; ++i)
  //      cout << argv[i] << "\n";
	switch(argv[1])
	{
		case 1 : sum = argv[0] - argv[2];
			cout << sum << endl;
	}
	
